{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "GDB Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/webserv",
            // "args": ["${workspaceFolder}/congifs/default.conf"],
			"args": ["${workspaceFolder}/jkl"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Skip system libraries",
                    "text": "set sysroot /",
                    "ignoreFailures": true
                },
                {
                    "description": "Skip all standard library code",
                    "text": "skip -gfi /usr/include/c++/*",
                    "ignoreFailures": true
                },
                {
                    "description": "Set to break only on my code",
                    "text": "set unwindonsignal on",
                    "ignoreFailures": true
                }
            ],
            "logging": {
                "engineLogging": true,
                "trace": true,
                "traceResponse": true
            }
        }
    ]
}